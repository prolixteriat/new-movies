    "theme": "dark",
    "import": [
        {
            "name": "alexa-layouts",
            "version": "1.5.0"
        }
    ],
    "mainTemplate": {
        "parameters": [
            "launchData"
        ],
        "items": [
            {
                "type": "AlexaHeadline",
                "primaryText": "${launchData.primaryText}",
                "secondaryText": "${launchData.secondaryText}",
                "backgroundColor": "@colorRed800",
                "footerHintText": "${launchData.footerHintText}"
            }
        ]
    }
}
